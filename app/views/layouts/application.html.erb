<!DOCTYPE html>
<html>
  <head>
    <title>beatmaps</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= render 'shared/navbar' %>
    <div class="flash-toaster green darken-1 z-depth-3"><%= notice %></div>
    <div class="flash-toaster deep-orange darken-4 z-depth-3"><%= alert %></div>
    <%= yield %>
  </body>
</html>
